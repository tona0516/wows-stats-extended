{
  "name": "wows-stats-extended",
  "version": "1.2.2",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "build": "run-s build:clean build:tsc build:generate-binary",
    "build:clean": "rm -rf dist/*",
    "build:tsc": "tsc --build",
    "build:generate-binary": "ts-node-dev generate_binary.js",
    "lint": "run-s lint:tsc lint:eslint lint:prettier lint:pug-lint",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:prettier": "prettier --check 'src/**/*.ts'",
    "lint:pug-lint": "pug-lint resource/**/*.pug",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "eslint 'src/**/*.ts' --fix",
    "fix:prettier": "prettier --write 'src/**/*.ts'",
    "run:development": "cross-env NODE_ENV=development ts-node-dev src/app.ts",
    "run:production": "cross-env NODE_ENV=production ts-node src/app.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tona0516/wows-stats-extended.git"
  },
  "author": "tona0516",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tona0516/wows-stats-extended/issues"
  },
  "homepage": "https://github.com/tona0516/wows-stats-extended#readme",
  "dependencies": {
    "@types/async": "^3.2.16",
    "@types/cheerio": "^0.22.31",
    "@types/express": "^4.17.14",
    "@types/node-persist": "^3.1.3",
    "@types/pug": "^2.0.6",
    "async": "^3.2.3",
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.10",
    "express": "^4.17.3",
    "json5": "^2.2.0",
    "log4js": "^6.4.3",
    "node-persist": "^3.1.0",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "eslint-plugin-import": "^2.26.0",
    "nexe": "^4.0.0-rc.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "pug-lint": "^2.6.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3"
  }
}
